{
  "name": "vnojs",
  "version": "1.1.6",
  "description": "vnojs,dymatic class,dymatic style",
  "main": "lib/vno.js",
  "bin": {
    "vno": "./bin/start"
  },
  "directories": {
    "test": "tests"
  },
  "exports": {
