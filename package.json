{
  "name": "vnojs",
  "version": "1.1.7",
  "description": "vnojs,dymatic class,dymatic style",
  "main": "./dist/index.cjs",
  "bin": {
    "vno": {
      "import": "./bin/vno.mjs",
      "require": "./bin/vno.cjs"
    }
  },
  "directories": {
    "test": "tests"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": [
      {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
      },
      "./dist/index.cjs"
    ]
  },
  "type": "module",
  "module": "./dist/index.mjs",
  "scripts": {
    "test": "node test.js",
    "rollup": "rollup --config"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yinyangshibolange/vnojs.git"
  },
  "keywords": [
    "dymatic",
    "class",
    "css",
    "style",
    "html"
  ],
  "files": [
    "dist/",
    "bin/",
    "config/"
  ],
  "author": "hongyu",
  "license": "ISC",
  "dependencies": {
    "chalk": "^5.3.0",
    "node-watch": "^0.7.4",
    "yargonaut": "^1.1.4",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3",
    "rollup": "^3.29.3"
  }
}
